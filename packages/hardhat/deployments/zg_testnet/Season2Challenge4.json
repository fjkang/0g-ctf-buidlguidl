ructor(address _nftContract) {\\n        nftContract = _nftContract;\\n    }\\n\\n    function mintFlag() external {\\n        _paid = false;\\n\\n        (bool ok, ) = msg.sender.call(\\\"\\\");\\n        require(ok, \\\"callback failed\\\");\\n        require(_paid, \\\"not paid\\\");\\n\\n        INFTFlags(nftContract).mint(tx.origin, 4);\\n\\n        _paid = false;\\n    }\\n\\n    receive() external payable {\\n        require(msg.value == EXPECTED_WEI, \\\"bad amount\\\");\\n        _paid = true;\\n    }\\n}\\n\",\"keccak256\":\"0x4443105fe6adf830a2de50774afc027dc8f5ab0f5a7d786d2072a93f22a41789\",\"license\":\"MIT\"}},\"version\":1}",
